{
    "name": "sierratecnologia/payment-php",
    "type": "library",
    "keywords": [
        "SierraTecnologia",
        "sitec",
        "api",
        "sdk",
        "payment processing",
        "api"
    ],
    "homepage": "https://sierratecnologia.com.br",
    "license": "MIT",
    "authors": [
        {
            "name": "Sierra Tecnologia",
            "email": "contato@sierratecnologia.com.br",
            "homepage": "https://sierratecnologia.com.br",
            "role": "Organizer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
      "phpunit/phpunit": ">=4.0",
      "php-coveralls/php-coveralls": ">=1.0",
      "squizlabs/php_codesniffer": ">=2.0",
      "symfony/process": ">=2.8",
        "phpro/grumphp": "^1.3",
        "psalm/plugin-laravel": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "SierraTecnologia\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
